                <script type="text/javascript">

                    $(document).ready(function () {
                        //States|USmap show hide logic..
                        var url = window.location.href;
                        if (url.toLowerCase().indexOf('/cox-') > -1) {
                            // Set TFN value
                            var TFN_value = window.localStorage.getItem('TFN');
                            if (TFN_value != null) {
                                $('.tfn').html(TFN_value);
                                $('.tfn_bottom').html(TFN_value);
                                $('.tfnicon').attr("href", "tel:+" + TFN_value);
                                if ($('.tfn-mob') != 'undefined') {
                                    $('.tfn-mob').children('a').attr("href", "tel:+" + TFN_value).html(TFN_value);
                                }
                                if ($('.tfn_md') != 'undefined') {
                                    $('.tfn_md').children('a').attr("href", "tel:+" + TFN_value).html(TFN_value);
                                }
                            }
                            $('.head01').hide();
                            $('.head02').hide();
                            $('.map_panel').hide();
                            $('.state-links').show();
                            // Cities Accordian Sorting Alphabetically And Truncating States - START
                            var str1 = '';
                            var str2 = '';
                            if ($('#BreadCrumbs').children('a').length > 1) {
                                str1 = $('#BreadCrumbs').children('a')[0].text;
                                str2 = $('#BreadCrumbs').children('a')[1].text;
                            }
                            var siteMapDiv = $('.sitemap-txt');
                            var citiesAnchorTags = siteMapDiv.children('a');
                            citiesAnchorTags.sort(function (a, b) {
                                var compA = $(a).text().toUpperCase();
                                var compB = $(b).text().toUpperCase();
                                return (compA < compB) ? -1 : (compA > compB) ? 1 : 0;
                            });
                            for (var i = 0; i < citiesAnchorTags.length ; i++) {
                                citiesAnchorTags[i].text = citiesAnchorTags[i].text.replace(str1, '').replace(str2, '');
                            }
                            $(siteMapDiv).append(citiesAnchorTags);
                            // Cities Accordian Sorting Alphabetically And Truncating States - END
                            if ($('.sitemap-txt').children().length > 3) {
                                for (var i = 0; i < 3; i++) {
                                    $('.sl_links').append($('.sitemap-txt').children()[0]);
                                }
                            }
                        } else {
                            window.localStorage.setItem('TFN', $('.tfn').html());
                            $('.state-links').hide();
                            $('.map-section').show();
                            $('.head01').show();
                            $('.head02').show();
                        }
                        $('.citybtn button').on('click', function () {

                            $(".allcities").toggle();
                        });
                    });
                </script>
