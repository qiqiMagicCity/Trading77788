    function ToggleQualification(isAddressQual, isReset) {
        $('.reqDiv').trigger('click');
        $(".bB_Zip").show();
        $(".submitForm").show();

        if (isAddressQual) {
            $('.bB_Address').show();
            $('.bB_Apt').show();
            $(".bB_Address :input").attr('questionid', 'txtAddress');
