                    path = window.location.href;
                    path = path.slice(0, -1);
                }
                else if (path != '/') {
                    path = window.location.pathname;
                    var lchar = path.substr(path.length - 1);
                    if (lchar === '/') {
                        path = path.substr(0, path.length - 1);
