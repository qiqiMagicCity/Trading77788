            $('.bB_Address').hide();
            $('.bB_Apt').hide();
            $(".bB_Address :input").attr('questionid', 'js_txtAddress');
            $(".bB_Apt :input").attr('questionid', 'js_txtSuite');
            $(".bB_Address :input").val(null);
            $(".bB_Apt :input").val(null);
        }

        if (isReset) {
            $(".bB_Address :input").val(null);
            $(".bB_Apt :input").val(null);
            $(".bB_Zip :input").val(null);

       
            $("input.home-radio:checkbox").not("[type=hidden]").prop("checked", false);
        }
