        const guid = urlParams.get('guid');
        const cookieName = "asiansmaller#lang";  


			//Is there a guid
			if (guid) {
				//Has the disclaimer been accepted.  If not - put in a loop till it is
				var cookieVal = getCookie(cookieName);
				if (cookieVal == "") {

					var interval = setInterval(function () {
						var cookieVal = getCookie(cookieName);
						if (cookieVal != "") {
							clearInterval(interval);
							PlayKuluVideo(guid);
						}
					}, 100);
				}
				else {
					PlayKuluVideo(guid);
				}
			}
        };
        function PlayKuluVideo(guid) {
            KV.widget.widgets.forEach(function (item) {
                if (item.contents().html().indexOf(guid) > -1) {
                    item.api.command("playKulu");
                }
            });
        }

        function getCookie(cname) {
            var name = cname + "=";
            var decodedCookie = decodeURIComponent(document.cookie);
            var ca = decodedCookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }


