<script src="./common_rwd/lib/magnific-popup/jquery.magnific-popup.min.js"></script>
<script src="./common_rwd/lib/slick/slick.min.js"></script>
<script>
$('.slideWrap').slick({
	arrows: false,
	fade: true,
	autoplay: true,
	autoplaySpeed: 10000,
	pauseOnHover: false,
	pauseOnFocus: false,
	speed: 2000,
	dots: true
});
$('.popup-iframe').magnificPopup({
	type: 'iframe',
	mainClass: 'mfp-fade',
	removalDelay: 200,
	preloader: false
});
var scrollNewsEle = $('.scrollNews');
var scrollNewsPos = 0;
var newsnum = scrollNewsEle.find('li').length;

function scrollNews(){
	setTimeout(function(){
		scrollNewsPos ++;
		var height = scrollNewsEle.height();
		if( scrollNewsPos >= newsnum ){
			scrollNewsPos = 0;
		}
		scrollNewsEle.animate({scrollTop:scrollNewsPos * height}, 500, 'swing', function(){ scrollNews(); });
	},3000);
}
scrollNews();
</script>
</body>
