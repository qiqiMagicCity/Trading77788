					<script language="javascript" type="text/javascript">
	var features = {
		first : {
			image:	'https://s3.amazonaws.com/media.agricharts.com/sites/2098/Responsive Slides/Slide1Croppedb.JPG',
			text:	'<div class="mission"><h2>OUR MISSION<\/h2><h3>To return value to our members\' corn through the best cost production in the renewable fuels industry.<\/h3><div>&nbsp;<\/div><\/div>'
		},
		second : {
			image:	'',
			text:	''
		},
		third : {
			image:	'',
			text:	''
		},
		fourth : {
			image:	'',
			text:	''
		},
		fifth : {
			image:	'',
			text:	''
		}
	};

	var featureTo;

	function switchFeature(selName, clicked) {
		var nameAry = new Array();
		var i=0, selId=0;

		for (var name in features) {
			if (name == selName)
				selId = i;
			nameAry[i] = name;
			i++;
			document.getElementById(name + 'Feature').style.display = 'none';
			document.getElementById(name + 'Feature').style.visibility = 'hidden';
		}

		selId = (selId == nameAry.length -1) ? 0 : selId + 1;

		if (!clicked)
			featureTo = setTimeout("switchFeature('" + nameAry[selId] + "', false)", 9000);
		else
			clearTimeout(featureTo);

		document.getElementById(selName + 'Feature').style.display = 'block';
		document.getElementById(selName + 'Feature').style.visibility = 'visible';
	}

	function printFeatures() {
		for (var id in features) {
			document.write('<div id="' + id + 'Feature">');
			document.write('	<div class="floatLeft imageContainer">');
			document.write('		<a href="javascript:void();"><img src="' + features[id].image + '" /></a>');
			document.write('	</div>');
			document.write('	<div class="floatRight textContainer">');
			document.write('		<div class="floatRight counter">');
			var i=1;

			for(var id2 in features) {
				var imgname = (id == id2) ? i : i + '_on';
				document.write('<a href="javascript:switchFeature(\'' + id2 + '\', true);"><img src="//s3.amazonaws.com/images2.aws.barchart.com/barchart/' + imgname + '.gif" alt ="' + i + '" border="0" /></a>&nbsp;');
				i++;
			}
			document.write('		</div>');
			document.write('		<div class="floatLeft">');
			document.write(				features[id].text);
			document.write('		</div>');
			document.write('	</div>');
			document.write('</div>');

			var feat = document.getElementById(id + 'Feature');
			feat.style.display = 'none';
			feat.style.visibility = 'hidden';
		}
		switchFeature ('first', false);
	}
</script>
