{
  "rewrites": [
    {
      "source": "/data/(.*)",
      "destination": "/data/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/js/$1"
    },
    {
      "source": "/css/(.*)",
      "destination": "/css/$1"
    },
    {
      "source": "/logos/(.*)",
      "destination": "/logos/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}